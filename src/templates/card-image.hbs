{{#each this}}
<li class="container-items">
    <div class="container-before">
    <img src="{{images.0.url}}" alt="" class="container-image">
    </div>
    <h2 class="text-color text-title">{{name}}</h2>
    {{#dates}}
    {{#start}}
    <p class="text-color text">{{localDate}}</p>
    {{/start}}
    {{/dates}}
    
    <a href="{{_embedded.venues.0.url}}" class="text-color-item text-link link"
            target="_blank">
        <svg class="img-vector" width="7" height="10">
            <use href="./images/sprite.svg#icon-place"></use>
        </svg>
            {{_embedded.venues.0.name}}
        </a>
</li>
{{/each}}

